<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ck.qianqian">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name="com.ck.util.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".IndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".FindPwdActivity"/>
        <activity android:name=".CheckCenterActivity"/>
        <activity android:name=".CheckPhoneActivity"/>
        <activity android:name=".CheckIdActivity"/>
        <activity android:name=".CheckCardActivity"/>
        <activity android:name=".CheckContactActivity"/>
        <activity android:name=".credit.CreditHistoryActivity"/>
        <activity android:name=".credit.CreditDetailActivity"/>
        <activity android:name=".FeedbackActivity"/>
        <activity android:name=".CheckMsgActivity"/>
        <activity android:name=".UpdatePwdActivity"/>
        <activity android:name=".WebViewActivity"/>
        <activity android:name=".credit.GetCreditActivity"/>

    </application>

</manifest>